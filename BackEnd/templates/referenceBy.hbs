{{#each referenceBy}}
<div class="wrapper referenceby">
    <div class="header-main ">
        <div class="cstm-container">
            <div class="bg-gradient"></div>
            <div class="header-box header-box-fixed">
                <div class="row align-items-center">
                    {{! start header name }}
                    <div class="col box1">
                        <div class="title-box">
                            <h3 class="titlebx secondary-color fw-500 fs-20 secondary-font">
                                Reference By {{this.headerData.name}}
                            </h3>
                        </div>
                    </div>
                    {{! end header name }}
                    <div class="col box2">
                        <div className="logoright" style="max-width: 200px;">
                            <img src="{{logo "logo.png"}}">
                        </div>
                    </div>
                </div>
            </div>
            <div class="innerbody-page">
                {{! start reference information and questions }}
                {{#if (isObject this.refInfo) }}
                <div class="cardbx-main">
                    {{! start reference information }}
                    <div class="innercardbx-body">
                        <div class="table-cardbox">
                            <div class="table-title secondary-bgcolor">
                                <h3 class="titlebx fs-14 white-color fw-600 secondary-font">
                                    Reference Information
                                </h3>
                            </div>
                            <div class="cardbody-bxx">
                                <div class="info-bx">
                                    <div class="row ">
                                        {{#convertObjtoArr this.refInfo}}
                                        <div class="col-md-6">
                                            <div class="infocard-upper cstm-row">
                                                <span class="label-name titlebx fs-12 secondary-color fw-400">
                                                    {{checkQuestionType key}}
                                                </span>
                                                <span class="desc-name titlebx fs-12 secondary-color fw-500">
                                                    {{checkStringType value}}
                                                </span>
                                            </div>
                                        </div>
                                        {{/convertObjtoArr}}
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    {{! end reference information }}
                    <div class="footer-totalcount pb-15">
                        <div class="countbox table-cardbox">
                            <ul class="count-listing">
                                <li>
                                    <div class="count-inner">
                                        <span class="count-num">1-3 = </span>
                                        <span class="count-name light-green-color">Average </span>
                                        <span class="count-num">( &gt;50% )</span>
                                    </div>
                                </li>
                                <li>
                                    <div class="count-inner">
                                        <span class="count-num">4-5 = </span>
                                        <span class="count-name dark-green-color">Good </span>
                                        <span class="count-num">( Top 50% )</span>
                                    </div>
                                </li>
                                <li>
                                    <div class="count-inner">
                                        <span class="count-num">6-7 = </span>
                                        <span class="count-name see-green-color">Very Good </span>
                                        <span class="count-num">( Top 25% )</span>
                                    </div>
                                </li>
                                <li>
                                    <div class="count-inner">
                                        <span class="count-num">8-9 = </span>
                                        <span class="count-name blue-color">Excellent</span>
                                        <span class="count-num">( Top 10% )</span>
                                    </div>
                                </li>
                                <li>
                                    <div class="count-inner">
                                        <span class="count-num">10 = </span>
                                        <span class="count-name purple-color">Outstanding</span>
                                        <span class="count-num">( Top 2% )</span>
                                    </div>
                                </li>
                            </ul>
                        </div>
                    </div>
                    {{! start reference questions }}
                    {{#if (isObject this.refInfo.questions.length) }}
                    <div class="innercardbx-body">
                        <div class="table-cardbox">
                            <div class="table-title secondary-bgcolor">
                                <h3 class="titlebx fs-14 white-color fw-600 secondary-font">
                                    Reference Questions
                                </h3>
                            </div>
                            <div class="cardbody-bxx">
