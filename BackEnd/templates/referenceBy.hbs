                                    <div class="innercard-detail-bx">
                                        <div class="titlebx fs-12 fw-500 grey-color boder-bottom-grey">
                                            {{question}}
                                        </div>
                                        <p class="subtitle fs-12 fw-400 secondary-color text-break">{{answer}}</p>
                                    </div>
                                </div>
                                {{/each}}
                            </div>
                        </div>
                    </div>
                    {{/if}}
                    {{! end reference questions }}
                </div>
                {{/if}}
